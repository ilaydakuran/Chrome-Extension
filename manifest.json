{
  "name": "Misogynistic Language Detector",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "For a more peaceful and happy world",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/3799277.png",
    "48": "icons/3799277.png",
    "128": "icons/3799277.png"
  },
  "default_locale": "en",
  "background": {
    
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/3799277.png",
    "default_title": "MLD has access to this site",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://instagram.com/*",
        "https://www.instagram.com/*"
      ],
      "js" : [
        "jquery-3.6.0.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "chrome://favicon/",
    "tabs",
    "activeTab", 
    "scripting"
  ]
}